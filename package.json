{
  "name": "studify-server",
  "version": "0.0.0",
  "description": "A server for studify-app built with TypeScript",
  "main": "build/index.js",
  "scripts": {
    "build-server": "tsc",
    "load-client": "git clone https://github.com/kekland/studify-web-app client",
    "build-client": "cd client && npm install && npm run build",
    "build": "npm run load-client && npm run build-client && npm run build-server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "node build/index.js",
    "start": "npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "github.com/kekland/studify-server"
  },
  "keywords": [
    "typescript",
    "server",
    "express",
    "socket.io"
  ],
  "author": "kekland <kk.erzhan@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.18.2",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^2.3.0",
    "typeorm": "^0.3.0-rc.19"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/chalk": "^2.2.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/socket.io": "^2.1.4",
    "typescript": "^3.8.3"
  }
}
